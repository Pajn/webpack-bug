    "@types/webpack-env": "^1.13.6",
    "apollo-link-batch-http": "^1.2.2",
    "apollo-link-dedup": "^1.0.9",
    "apollo-link-error": "^1.1.0",
    "apollo-link-ws": "^1.0.8",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "subscriptions-transport-ws": "^0.9.14"
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "@babel/core": "7.0.0-rc.1",
    "@babel/runtime": "7.0.0-rc.1",
    "@svgr/webpack": "^2.2.1",
    "autoprefixer": "9.1.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "23.4.2",
    "babel-loader": "8.0.0-beta.4",
    "babel-plugin-dynamic-import-node": "^2.0.0",
    "babel-plugin-named-asset-import": "1.0.0-next.3e165448",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-preset-react-app": "https://github.com/bappogroup/babel-preset-react-app#5067370a35f05b8aea05e22932157d256e35cd68",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "chalk": "2.4.1",
    "commander": "^2.17.1",
    "cross-spawn": "^6.0.5",
    "css-loader": "1.0.0",
    "dotenv": "6.0.0",
    "dotenv-expand": "4.2.0",
    "file-loader": "1.1.11",
    "fork-ts-checker-notifier-webpack-plugin": "0.4.0",
    "fork-ts-checker-webpack-plugin": "0.4.9",
    "fs-extra": "7.0.0",
    "graphql": "0.13.2",
    "graphql-tag": "2.9.2",
    "html-webpack-plugin": "^4.0.0-alpha",
    "hyperquest": "^2.1.2",
    "jest": "23.5.0",
    "jest-cli": "23.5.0",
    "merge2": "1.2.2",
    "mini-css-extract-plugin": "^0.4.2",
    "nodemon": "1.18.3",
    "object-assign": "4.1.1",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "promise": "8.0.1",
    "react-dev-utils": "6.0.0-next.3e165448",
    "semver": "^5.5.1",
    "style-loader": "0.22.1",
    "sw-precache-webpack-plugin": "0.11.5",
    "tar-pack": "^3.4.0",
    "thread-loader": "1.2.0",
    "tmp": "0.0.33",
    "ts-jest": "23.1.4",
    "ts-loader": "4.5.0",
    "typescript": "~3.0.1",
    "uglifyjs-webpack-plugin": "1.3.0",
    "url-loader": "1.1.1",
    "validate-npm-package-name": "^3.0.0",
    "webpack": "4.17.0",
    "webpack-dev-server": "3.1.5",
    "webpack-manifest-plugin": "2.0.3",
    "whatwg-fetch": "2.0.4"
  },
  "tscomp": {
    "mode": "browser"
  },
  "scripts": {
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "start": "node scripts/start.js"
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">0.25%",
      "not op_mini all",
      "ie 11"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.[tj]s?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).[tj]s?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/dist/index.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "ts",
      "tsx",
      "node"
    ]
  }
}
